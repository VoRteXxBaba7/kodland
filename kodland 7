import random
emojis = ["^^", "0_o", ":)", "¯\\_(ツ)_/¯", "(￢_￢)"]
print(random.choice(emojis))

hakkimdaki_gercekler=["tarçın","tavuk pilav","araba projem"]
print(random.choice(hakkimdaki_gercekler))
